  "device_integrations": {
    "supported_devices": [
      {
        "id": "apple-watch",
        "name": "Apple Watch",
        "brand": "Apple",
        "type": "wearable",
        "metrics": ["Heart Rate", "Steps", "Sleep", "Workouts"],
        "description": "Heart rate, activity, and sleep tracking"
      },
      {
        "id": "freestyle-libre",
        "name": "FreeStyle Libre",
        "brand": "Abbott",
        "type": "cgm",
        "metrics": ["Blood Glucose", "Trends", "Alerts"],
        "description": "Continuous glucose monitoring"
      }
    ]
  },
  "api_endpoints": {
    "openai_chat": {
      "path": "/openai-chat",
      "method": "POST",
      "description": "Send message to AI health coach",
      "required_fields": ["message", "userId"],
      "optional_fields": ["sessionId"]
    },
    "nutrition_analysis": {
      "path": "/nutrition-analysis",
      "method": "POST",
      "description": "Analyze food nutrition and health impact",
      "required_fields": ["foodName", "userId"],
      "optional_fields": ["quantity", "mealType"]
    },
    "elevenlabs_tts": {
      "path": "/elevenlabs-tts",
      "method": "POST",
      "description": "Convert text to speech",
      "required_fields": ["text"],
      "optional_fields": ["voiceId", "settings"]
    },
    "spoonacular_recipes": {
      "path": "/spoonacular-recipes",
      "method": "GET",
      "description": "Search for health-optimized recipes",
      "query_params": ["query", "diet", "intolerances", "maxReadyTime", "number"]
    }
  },
  "database_schema": {
    "main_tables": [
      {
        "name": "profiles",
        "description": "User profile information",
        "key_fields": ["id", "email", "first_name", "last_name", "onboarding_completed"]
      },
      {
        "name": "health_metrics",
        "description": "Health data from various sources",
        "key_fields": ["user_id", "metric_type", "value", "unit", "timestamp", "source"]
      },
      {
        "name": "chat_sessions",
        "description": "AI coach conversation sessions",
        "key_fields": ["user_id", "title", "message_count", "last_message"]
      },
      {
        "name": "chat_history",
        "description": "Individual chat messages",
        "key_fields": ["user_id", "session_id", "message", "response", "role"]
      },
      {
        "name": "food_logs",
        "description": "Nutrition tracking data",
        "key_fields": ["user_id", "food_name", "meal_type", "calories", "glucose_impact"]
      },
      {
        "name": "supplements",
        "description": "Available supplement products",
        "key_fields": ["name", "description", "price", "category", "tier", "evidence_rating"]
      },
      {
        "name": "fertility_tracking",
        "description": "UBERGENE fertility data",
        "key_fields": ["user_id", "cycle_day", "basal_temperature", "cycle_phase"]
      }
    ]
  },
